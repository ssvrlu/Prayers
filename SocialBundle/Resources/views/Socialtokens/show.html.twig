{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Socialtokens</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Accessforuserid</th>
                <td>{{ entity.accessForUserId }}</td>
            </tr>
            <tr>
                <th>Accessto</th>
                <td>{{ entity.accessTo }}</td>
            </tr>
            <tr>
                <th>Token</th>
                <td>{{ entity.token }}</td>
            </tr>
            <tr>
                <th>Expiresat</th>
                <td>{{ entity.expiresAt|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('socialtokens') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
