<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Socialtokens" table="socialtokens">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="accessForUserId" type="bigint" column="access_for_user_id" nullable="false"/>
    <field name="accessTo" type="string" column="access_to" length="32" nullable="false"/>
    <field name="token" type="string" column="token" length="255" nullable="false"/>
    <field name="expiresAt" type="datetime" column="expires_at" nullable="false"/>
  </entity>
</doctrine-mapping>
