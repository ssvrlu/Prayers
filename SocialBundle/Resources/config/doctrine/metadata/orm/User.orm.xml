<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="User" table="user">
    <id name="objectId" type="bigint" column="object_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="accountId" type="integer" column="account_id" nullable="false"/>
    <field name="roles" type="text" column="roles" nullable="false"/>
    <field name="invitationUsed" type="string" column="invitation_used" length="255" nullable="true"/>
    <field name="firstName" type="string" column="first_name" length="255" nullable="false"/>
    <field name="lastName" type="string" column="last_name" length="255" nullable="false"/>
    <field name="pictureId" type="integer" column="picture_id" nullable="true"/>
    <field name="coverId" type="integer" column="cover_id" nullable="true"/>
    <field name="dateOfBirth" type="datetime" column="date_of_birth" nullable="false"/>
    <field name="gender" type="string" column="gender" length="1" nullable="false"/>
    <field name="church" type="string" column="church" length="255" nullable="true"/>
    <field name="city" type="string" column="city" length="255" nullable="true"/>
    <field name="facebookId" type="integer" column="facebook_id" nullable="true"/>
    <field name="actingAsPageId" type="bigint" column="acting_as_page_id" nullable="true"/>
    <field name="public" type="integer" column="public" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false"/>
  </entity>
</doctrine-mapping>
